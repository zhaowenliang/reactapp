{
    "compilerOptions": {
        "jsx": "react-native",
        "target": "esnext",
        "module": "commonjs",
        "experimentalDecorators": true,
        "allowJs": true,
        "strict": true,
        "importHelpers": true,
        "baseUrl": "src",
        "paths": {
            "*": [
                "/*"
            ],
            "@Image/*": [
                "image/*"
            ],
            "@Router/*": [
                "route/*"
            ],
            "@Study/*": [
                "study/*"
            ],
            "@StudyNavigation/*": [
                "study_navigation/*"
            ],
        }
    }
}