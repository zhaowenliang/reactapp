{
    "compilerOptions": {
        "module": "commonjs",
        "experimentalDecorators": true,
        "baseUrl": "src",
        "paths": {
            "*": [
                "/*"
            ],
            "@Image/*": [
                "image/*"
            ],
            "@Router/*": [
                "route/*"
            ],
            "@Study/*": [
                "study/*"
            ],
            "@StudyNavigation/*": [
                "study_navigation/*"
            ],
        }
    },
    "exclude": [
        "node_modules"
    ]
}